user www-data;
worker_processes auto;
pid /run/nginx.pid;
include /etc/nginx/modules-enabled/*.conf;

stream {
    server {
        listen     443;
        proxy_pass localhost:31443;
    }
    server {
        listen     80;
        proxy_pass localhost:31080;
    }
}

events {
    worker_connections  1024;
}
